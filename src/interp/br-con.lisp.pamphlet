(defun |htSayArgument| (|t|)
  (PROG (|k| |typeOfArg|)
  (declare 
   (special |$signature| |$conkind| |$conlength| |$generalSearch?| |$conargs|))
    (RETURN
      (COND
        ((NULL |$signature|)
         (|htSay| (CONS "{\\em "
                        (CONS |t| (CONS "}" NIL)))))
        ((member |t| '($ %))
         (COND
           ((AND (BOOT-EQUAL |$conkind| "category")
                 (> |$conlength| 20))
            (COND
              (|$generalSearch?|
                  (|htSay|                                "{\\em D} of the origin category"))
              (t (|addWhereList| '$ '|is| NIL)
               (|htSayStandard| "{\\em $}"))))
