         (setq |count| (|#| |lines|))
         (COND
           ((EQL |count| 0) (|emptySearchPage| kind |filter|))
           ((|member| kind '("attribute" "operation"))
            (|dbShowOperationLines| kind |lines|))
           (t (|dbShowConstructorLines| |lines|))))))))

