(defun |grepSplit| (|lines| |doc?|)
  (PROG (|instream2| CONS N |line| kind |cats| |doms| |defs| |paks|
            |atts| |ops|)
  (declare (special |$includeUnexposed?|))
    (RETURN
      (SEQ (PROGN
             (COND
               (|doc?| (setq |instream2|
                                (OPEN (STRCONC
                                       (|getEnv| "AXIOM")
