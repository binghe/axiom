                              G174461)
                           (SEQ (EXIT (SETQ G174461
                                       (OR G174461
                                        (BOOT-EQUAL (ELT |s| |i|)
                                         #\&)))))))))
                (setq |key| '|y|)))
             (setq |filter| (|pmTransFilter| (princ-to-string |s|)))
             (COND
               ((AND (CONSP |filter|) (EQ (QCAR |filter|) '|error|))
                |filter|)
               (t (setq |pattern| (|mkGrepPattern| |filter| |key|))
