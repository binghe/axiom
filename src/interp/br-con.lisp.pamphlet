(defun |escapeSpecialChars| (|s|)
  (PROG (|u| |buf|)
  (declare (special |$htSpecialChars| |$htCharAlist|))
    (RETURN
      (SEQ (COND
             ((setq |u| (LASSOC |s| |$htCharAlist|)) |u|)
             ((|member| |s| |$htSpecialChars|)
              (STRCONC "\\" |s|))
             ((ALPHA-CHAR-P (ELT |s| 0)) |s|)
             ((NULL (PROG (G167323)
                      (setq G167323 nil)
                      (RETURN
                        (DO ((G167329 nil G167323)
                             (G167330 (MAXINDEX |s|))
                             (|i| 0 (QSADD1 |i|)))
                            ((OR G167329 (QSGREATERP |i| G167330))
                             G167323)
