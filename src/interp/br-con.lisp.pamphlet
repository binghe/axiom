;  atom s => s
;  [unTab1 first s, :rest s]

(defun |unTab| (|s|)
  (COND
    ((STRINGP |s|) (|unTab1| |s|))
    ((ATOM |s|) |s|)
    (t (CONS (|unTab1| (CAR |s|)) (CDR |s|)))))

;unTab1 s ==
;  STRING_<('"\tab{", s) = 5 and (k := charPosition(char '_}, s, 4)) =>
;      SUBSTRING(s, k + 1, nil)
;  s

(defun |unTab1| (|s|)
  (PROG (|k|)
    (RETURN
      (COND
        ((AND (EQL (STRING< "\\tab{" |s|) 5)
              (setq |k| (|charPosition| #\} |s| 4)))
         (SUBSTRING |s| (+ |k| 1) NIL))
        (t |s|)))))

