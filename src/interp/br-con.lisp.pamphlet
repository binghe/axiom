                            (PROGN
                              (setq |op| (CAR G173914))
                              (setq |alist| (CDR G173914))
                              G173914)
                            NIL))
                      (NREVERSE0 G173925))
                   (SEQ (EXIT (COND
                                ((setq |nalist|
