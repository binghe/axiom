;  s

(defun |replaceTicksBySpaces| (|s|)
  (PROG (|max| |n|)
    (RETURN
      (SEQ (PROGN
             (setq |n| (- 1))
             (setq |max| (MAXINDEX |s|))
             (DO ()
                 ((NULL (<= (setq |n|
                                     (|charPosition| #\` |s|
                                      (+ |n| 1)))
                            |max|))
                  NIL)
               (SEQ (EXIT (SETELT |s| |n| #\space))))
             |s|)))))

