  (PROG (|$predicateList| |domname| |conname| |ops| |op| |sig|)
    (DECLARE (SPECIAL |$predicateList|))
    (RETURN
      (SEQ (PROGN
             (setq |domname| (ELT D 0))
             (setq |conname| (CAR |domname|))
             (setq |$predicateList|
                      (GETDATABASE |conname| 'PREDICATES))
             (setq |ops| (ifcar |option|))
             (PROG (G170045)
               (setq G170045 NIL)
               (RETURN
                 (DO ((G170051 (|getDomainSigs1| D |ops|)
                          (CDR G170051))
                      (G170035 NIL))
                     ((OR (ATOM G170051)
                          (PROGN (SETQ G170035 (CAR G170051)) NIL)
                          (PROGN
                            (PROGN
                              (setq |op| (CAR G170035))
                              (setq |sig| (CADR G170035))
                              G170035)
                            NIL))
                      (NREVERSE0 G170045))
                   (SEQ (EXIT (SETQ G170045
                                    (CONS
                                     (CONS |op|
                                      (CONS |sig|
                                       (|getInheritanceByDoc| D |op|
                                        |sig|)))
                                     G170045))))))))))))

