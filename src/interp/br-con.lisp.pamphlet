             (setq CONS (setq |atts| (setq |doms| NIL)))
             (DO ()
                 ((NULL (AND (CONSP |lines|)
                             (PROGN
                               (setq |line| (QCAR |lines|))
                               (setq |lines| (QCDR |lines|))
                               t)))
                  NIL)
               (SEQ (EXIT (PROGN
                            (COND
                              (|doc?| (setq N
                                       (PARSE-INTEGER
                                        (|dbPart| |line| 1
                                         (- 1))))
