(defun |getExtensionsOfDomain| (|domain|)
  (PROG (|u| |cats|)
    (RETURN
      (SEQ (PROGN
             (setq |u| (|getDomainExtensionsOfDomain| |domain|))
             (setq |cats| (|getCategoriesOfDomain| |domain|))
             (DO ((G170078 |u| (CDR G170078)) (|x| NIL))
                 ((OR (ATOM G170078)
                      (PROGN (SETQ |x| (CAR G170078)) NIL))
                  NIL)
               (SEQ (EXIT (setq |cats|
                                   (|union| |cats|
