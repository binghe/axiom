                                          (setq |r| (CDR |item|))
                                          (COND
                                            ((NULL |r|)
                                             (CONS |sig| (CONS t NIL)))
                                            (t
                                             (setq |key| (CAR |r|))
                                             (setq |options|
                                              (CDR |r|))
                                             (COND
                                               ((NULL
                                                 (setq |pred|
                                                  (IFCAR |options|)))
                                                (COND
                                                  ((BOOT-EQUAL
                                                    (IFCAR
                                                     (IFCDR |options|))
                                                    'ASCONST)
                                                   (CONS |sig|
                                                    (CONS 'ASCONST NIL)))
                                                  (t
                                                   (CONS |sig|
                                                    (CONS t NIL)))))
                                               ((setq |npred|
                                                 (|simpHasPred| |pred|))
                                                (CONS |sig|
                                                 (CONS |npred| NIL)))
                                               (t NIL))))))
                               (SETQ G173969 (CONS |x| G173969)))))))))))))

