                              ((OR (ATOM G179518)
                                   (PROGN
                                     (SETQ |a| (CAR G179518))
                                     NIL)
                                   (ATOM G179519)
                                   (PROGN
                                     (SETQ |t| (CAR G179519))
                                     NIL))
                               NIL)
                            (SEQ (EXIT (PROGN
