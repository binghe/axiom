;--------------------> NEW DEFINITION (see br-op2.boot.pamphlet)
;htSayIndentRel(n,:options) ==
;  flag := IFCAR options
;  m := ABSVAL n
;  if flag then m := m + 2
;  htSayStandard
;    n > 0 =>
;      flag => ['"\indent{",princ-to-string m,'"}\tab{-2}"]
;      ['"\indent{",princ-to-string m,'"}\tab{0}"]
;    n < 0 => ['"\indent{0}\newline "]

(defun |htSayIndentRel| (&REST G179673 &AUX |options| |n|)
  (DSETQ (|n| . |options|) G179673)
  (PROG (|flag| |m|)
    (RETURN
      (PROGN
        (setq |flag| (IFCAR |options|))
        (setq |m| (abs |n|))
        (COND (|flag| (setq |m| (+ |m| 2))))
        (|htSayStandard|
                  (COND
                    ((> |n| 0)
                     (COND
                       (|flag| (CONS "\\indent{"
                                     (CONS (princ-to-string |m|)
                                      (CONS "}\\tab{-2}"
                                       NIL))))
                       (t
                        (CONS "\\indent{"
                              (CONS (princ-to-string |m|)
                                    (CONS "}\\tab{0}" NIL))))))
                    ((MINUSP |n|)
                     (CONS "\\indent{0}\\newline " NIL))))))))


