                                        ((NUMBERP N)
                                         (FILE-POSITION |instream2| N)
                                         (setq |line|
                                          (read-line |instream2|)))
                                        (t NIL))))
                            (setq kind (|dbKind| |line|))
                            (COND
                              ((AND (NULL |$includeUnexposed?|)
                                    (NULL (|dbExposed?| |line| kind)))
                               '|skip|)
                              ((AND (OR
                                     (BOOT-EQUAL kind #\a)
                                     (BOOT-EQUAL kind #\o))
                                    (|isDefaultOpAtt| |line|))
                               '|skip|)
                              ((BOOT-EQUAL kind #\c)
                               (setq |cats|
                                        (|insert| |line| |cats|)))
                              ((BOOT-EQUAL kind #\d)
                               (setq |doms|
                                        (|insert| |line| |doms|)))
                              ((BOOT-EQUAL kind #\x)
                               (setq |defs|
                                        (|insert| |line| |defs|)))
                              ((BOOT-EQUAL kind #\p)
                               (setq |paks|
                                        (|insert| |line| |paks|)))
                              ((BOOT-EQUAL kind #\a)
                               (setq |atts|
                                        (|insert| |line| |atts|)))
                              ((BOOT-EQUAL kind #\o)
                               (setq |ops| (|insert| |line| |ops|)))
                              ((BOOT-EQUAL kind #\-) '|skip|)
                              (t (|systemError| '|kind|)))))))
             (COND (|doc?| (CLOSE |instream2|)))
             (CONS (CONS "attribute" (NREVERSE |atts|))
                   (CONS (CONS "operation"
                               (NREVERSE |ops|))
                         (CONS (CONS "category"
                                     (NREVERSE |cats|))
                               (CONS (CONS "domain"
                                      (NREVERSE |doms|))
                                     (CONS
                                      (CONS "package"
                                       (NREVERSE |paks|))
                                      NIL))))))))))

;mkUpDownPattern s == recurse(s,0,#s) where
;  recurse(s,i,n) ==
;    i = n => '""
;    STRCONC(fixchar(s.i),recurse(s,i + 1,n))
;  fixchar(c) ==
;    ALPHA_-CHAR_-P c =>
;      STRCONC(char '_[,CHAR_-UPCASE c,CHAR_-DOWNCASE c,char '_])
;    c

(defun |mkUpDownPattern,fixchar| (|c|)
  (SEQ (IF (ALPHA-CHAR-P |c|)
           (EXIT (STRCONC #\[ (CHAR-UPCASE |c|)
                          (CHAR-DOWNCASE |c|) #\])))
       (EXIT |c|)))

(defun |mkUpDownPattern,recurse| (|s| |i| |n|)
  (SEQ (IF (BOOT-EQUAL |i| |n|) (EXIT ""))
       (EXIT (STRCONC (|mkUpDownPattern,fixchar| (ELT |s| |i|))
                      (|mkUpDownPattern,recurse| |s| (+ |i| 1) |n|)))))

(defun |mkUpDownPattern| (|s|)
  (|mkUpDownPattern,recurse| |s| 0 (|#| |s|)))

