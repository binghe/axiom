(defun |removeSurroundingStars| (|filter|)
  (PROG (|max| |key|)
    (RETURN
      (PROGN
        (setq |key| (princ-to-string |filter|))
        (COND
          ((BOOT-EQUAL (ELT |key| 0) #\*)
