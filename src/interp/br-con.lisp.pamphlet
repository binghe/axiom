                                                  NIL))
                                               NIL)
                                               (SEQ
                                                (EXIT
                                                 (COND
                                                   ((ELT |x| 3)
                                                    (setq |acc|
                                                     (CONS |x| |acc|)))))))
                                             (COND
                                               ((NULL |acc|) NIL)
                                               (t
                                                (CONS |op|
                                                 (NREVERSE |acc|)))))
                                           (SETQ G170705
                                            (CONS
                                             (PROGN
                                               (setq |acc| NIL)
                                               (DO
                                                ((G170736 |items|
                                                  (CDR G170736))
                                                 (|x| NIL))
                                                ((OR (ATOM G170736)
                                                  (PROGN
                                                    (SETQ |x|
                                                     (CAR G170736))
                                                    NIL))
                                                 NIL)
                                                 (SEQ
                                                  (EXIT
                                                   (COND
                                                     ((ELT |x| 3)
                                                      (setq |acc|
                                                       (CONS |x| |acc|)))))))
                                               (COND
                                                 ((NULL |acc|) NIL)
                                                 (t
                                                  (CONS |op|
                                                   (NREVERSE |acc|)))))
                                             G170705))))))))))))
                (setq |$conformsAreDomains|
                         (|htpProperty| |htPage| '|domname|))
                (setq |opCount| (|opAlistCount| |opAlist| |which|))
                (setq |branch|
                         (COND
                           ((INTEGERP |key|)
                            (COND
                              ((<= |opCount| |$opDescriptionThreshold|)
                               '|documentation|)
                              (t '|names|)))
                           ((AND (BOOT-EQUAL |key| '|names|)
                                 (NULL (CDR |opAlist|)))
                            (COND
                              ((<= |opCount| |$opDescriptionThreshold|)
                               '|documentation|)
                              (t '|names|)))
                           (t |key|)))
                (setq |LETTMP#1| (LASSOC |branch| |$OpViewTable|))
                (setq |what| (CAR |LETTMP#1|))
                (setq |whats| (CADR |LETTMP#1|))
                (setq |fn| (CADDR |LETTMP#1|))
                (setq |data|
                         (|dbGatherData| |htPage| |opAlist| |which|
                             |branch|))
                (setq |dataCount|
                         (PROG (G170742)
                           (setq G170742 0)
                           (RETURN
                             (DO ((G170748 |data| (CDR G170748))
                                  (|x| NIL))
                                 ((OR (ATOM G170748)
                                      (PROGN
                                        (SETQ |x| (CAR G170748))
                                        NIL))
                                  G170742)
