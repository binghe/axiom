                                        (NREVERSE0 G179491))
                                        (SEQ
                                         (EXIT
                                          (COND
                                            ((IDENTP |x|)
                                             (SETQ G179491
                                              (CONS |x| G179491))))))))))
                         (setq |$DomainList|
                                  (SETDIFFERENCE |$DomainList|
                                      |symbolsUsed|))
                         (|getSubstSigIfPossible| |sig|))))
             (COND
               ((|member| |which| '("operation" "constructor"))
