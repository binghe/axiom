                            (PRINC |prefix| |outstream|)
                            (PRINC |$tick| |outstream|)
                            (COND
                              ((NULL comments)
                               (PRINC 0 |outstream|)
                               (TERPRI |outstream|))
                              (t (PRINC |comP| |outstream|)
