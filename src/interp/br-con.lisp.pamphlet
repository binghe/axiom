        ((NULL (IDENTP |t|)) (|bcConform| |t| t))
        (t (setq |k| (|position| |t| |$conargs|))
         (COND
           ((> |k| (- 1))
            (setq |typeOfArg| (ELT (CDR |$signature|) |k|))
            (|addWhereList| |t| '|member| |typeOfArg|)))
         (|htSay| "{\\em " |t| "}"))))))

